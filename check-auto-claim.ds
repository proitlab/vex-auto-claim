#!/bin/bash

# Update WORKDIR to match your WORKDIR in script auto-claim-rewards.ds
WORKDIR=/root 
AUTOCLAIM=${WORKDIR}/auto-claim-rewards.ds

STATUS=`ps -ef | grep auto-claim-rewards | grep -v grep | wc -l`

if [ ${STATUS} -ne 1 ]
then
	echo `date` " Auto Claim ${BPNAME} is not running. Run ${AUTOCLAIM}"
	$AUTOCLAIM &
fi

